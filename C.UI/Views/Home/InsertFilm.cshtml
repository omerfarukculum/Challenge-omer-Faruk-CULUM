
@{
    ViewBag.Title = "InsertFilm";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="card col-md-8 mx-auto mt-5 mb-4">
    <h5 class="card-title text-center mt-3 fs-2">Film Ekle</h5>
    <div class="card-body">
        <form>


            <div class="input-group mb-3">
                <span class="input-group-text" id="inputGroup-sizing-default">Film Adı :</span>
                <input type="text" id="FilmAd" class="form-control" aria-label="Sizing example input" aria-describedby="inputGroup-sizing-default">

            </div>
            <div class="input-group mb-3">

                <span class="input-group-text" id="inputGroup-sizing-default">Film Yapım Yılı :</span>
                <input type="text" id="FilmYapimYil" class="form-control" aria-label="Sizing example input" aria-describedby="inputGroup-sizing-default">
            </div>
            <a href="#"id="InsertFilm" class=" btn btn-success">Kaydet</a>
        </form>
    </div>
</div>

   





